    "gulp-babel": "^6.1.2",
    "gulp-clean-css": "^2.0.3",
    "gulp-concat": "^2.4.3",
    "gulp-coveralls": "^0.1.3",
    "gulp-htmlmin": "^2.0.0",
    "gulp-imagemin": "^3.0.1",
    "gulp-rename": "^1.2.0",
    "gulp-rev-append": "^0.1.6",
    "gulp-sass": "^2.3.2",
    "gulp-typescript": "~3.1.4",
    "gulp-uglify": "^1.1.0",
    "gulp-util": "^3.0.3",
    "istanbul": "^0.4.4",
    "jasmine-core": "^2.2.0",
